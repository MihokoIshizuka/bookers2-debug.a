<table class="table table-hover table-inverse">
  <% if @range == "User" %>
  <h2>Users search for "#{search}"</h2>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
    </tr>
    <% end %>
  </tbody>
  <% else %>
  <h2>Books search for "#{search}"</h2>
  <tbody>
  <% @books.each do |book| %>
  <tr>
    <td><%= book.title %></td>
    <td><%= book.body %></td>
  </tr>
  </tbody>
  <% end %>
</table>